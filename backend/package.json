{
    "name": "rail-madad-ai-backend",
    "version": "1.0.0",
    "description": "Backend API for Rail Madad AI - Railway Complaint Management System",
    "main": "dist/server.js",
    "scripts": {
        "dev": "nodemon --exec ts-node src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "seed": "ts-node src/utils/seeder.ts",
        "lint": "eslint src/**/*.ts",
        "format": "prettier --write \"src/**/*.ts\"",
        "test": "echo \"Tests coming soon\" && exit 0"
    },
    "keywords": [
        "railway",
        "complaint",
        "management",
        "ai",
        "nodejs",
        "typescript"
    ],
    "author": "Rail Madad AI Team",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "mongoose": "^8.0.0",
        "jsonwebtoken": "^9.0.2",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express-validator": "^7.0.1",
        "express-rate-limit": "^7.1.5",
        "helmet": "^7.1.0",
        "morgan": "^1.10.0",
        "axios": "^1.6.2"
    },
    "devDependencies": {
        "typescript": "^5.3.2",
        "@types/node": "^20.9.0",
        "@types/express": "^4.17.21",
        "@types/cors": "^2.8.17",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/bcryptjs": "^2.4.6",
        "@types/morgan": "^1.9.9",
        "ts-node": "^10.9.1",
        "nodemon": "^3.0.1",
        "eslint": "^8.54.0",
        "@typescript-eslint/parser": "^6.12.0",
        "@typescript-eslint/eslint-plugin": "^6.12.0",
        "prettier": "^3.1.0"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    }
}